.maps#map

:javascript
  var map;
  function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: -34.397, lng: 150.644},
      zoom: 8
    });
  }
%script{ src: "https://maps.googleapis.com/maps/api/js?key=AIzaSyCFCuIXvM1YHlPcZiILdOdaFDBCAhHF8gk&callback=initMap", async: :defer }